<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="SVNStatistics">

    <typeAlias alias="" type="" />
    
     

    <insert id="insertStatistic" parameterClass="com.neosavvy.svn.analytics.dto.SVNStatistic">
        INSERT INTO 
            SVN_STATISTIC
        (
            AUTHOR
            ,REVISION
            ,REV_DATE
            ,MESSAGE
            ,NUM_FILES_IN_REV
            ,NUM_FILES_ADDED_IN_REV
            ,NUM_FILES_DELETED_IN_REV
            ,NUM_FILES_MODIFIED_IN_REV
        )
        VALUES
        (
            #author#
            ,#revision#
            ,#date#
            ,#message#
            ,#numFilesInRevision#
            ,#numFilesAddedInRevision#
            ,#numFilesDeletedInRevision#
            ,#numFilesModifiedInRevision#
        )
    </insert>   
  

</sqlMap>