<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
		http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">

    
    <bean id="abstractDao" abstract="true" class="org.springframework.orm.ibatis.support.SqlMapClientDaoSupport">
	    <property name="sqlMapClient" ref="sqlMapClient"/>
	</bean>    
    <bean parent="abstractDao" id="svnAnalyticsDAO" class="com.neosavvy.svn.analytics.dao.ibatis.IbatisSVNStatisticDAOImpl"/>
    <bean parent="abstractDao" id="svnRepositoryDAO" class="com.neosavvy.svn.analytics.dao.ibatis.IbatisSVNRepositoryDAOImpl"/>
	<bean parent="abstractDao" id="svnFileNodeStatisticDAO" class="com.neosavvy.svn.analytics.dao.ibatis.IbatisSVNFileStatisticDAOImpl"/>
	<bean parent="abstractDao" id="codeOwnershipDAO" class="com.neosavvy.svn.analytics.dao.ibatis.IbatisCodeOwnershipDAOImpl"/>
</beans>