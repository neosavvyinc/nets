<?xml version="1.0" ?>
<mx:HBox xmlns:mx="http://www.adobe.com/2006/mxml">

    <mx:Script>
        <![CDATA[

        import com.neosavvy.user.dto.project.ExpenseItem;
        import com.neosavvy.user.view.secured.expenses.report.event.ExpenseItemEvent;

        protected function editExpenseItemHandler():void {
            dispatchEvent(new ExpenseItemEvent(ExpenseItemEvent.ACTION_EDIT, data as ExpenseItem));
        }

        protected function deleteExpenseItemHandler():void {
            dispatchEvent(new ExpenseItemEvent(ExpenseItemEvent.ACTION_DELETE, data as ExpenseItem));
        }
        ]]>
    </mx:Script>
    <mx:Button id="editBtn" styleName="editLinkButton"  label="Edit" toolTip="Edit Expense Item" click="{editExpenseItemHandler()}"/>
    <mx:Button id="deleteBtn" styleName="deleteLinkButton" label="Delete" toolTip="Delete Expense Item" click="{deleteExpenseItemHandler()}"/>
</mx:HBox>