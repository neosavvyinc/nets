<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="1.0">

    <persistence-unit name="FocalStoragePU">
        <description>Focal Builds Persistence Unit</description>
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>jdbc/FocalDS</jta-data-source>
        
        <class>fineline.focal.users.types.v1.FocalSession</class>
        <class>fineline.focal.users.types.v1.InternalUser</class>
        <class>fineline.focal.users.types.v1.ExternalUser</class>
        <class>fineline.focal.users.types.v1.Role</class>

        <class>fineline.focal.common.types.v1.FileRef</class>
        <class>fineline.focal.common.types.v1.UrlFileRef</class>
        <class>fineline.focal.common.types.v1.StorageServiceFileRef</class>
        <properties>
            <property name="eclipselink.target-server" value="SunAS9"/> 
            <!-- <property name="eclipselink.session.customizer" value="fineline.focal.common.model.CacheCoordinationSessionCustomizer"/> -->
            <!-- property name="eclipselink.logging.level" value="FINEST"/-->
        </properties>
    </persistence-unit>
</persistence>

